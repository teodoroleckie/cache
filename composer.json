{
  "name": "tleckie/cache",
  "license": "LGPL-3.0-only",
  "description": "PSR-6 and PSR-16 Couchbase cache adapter",
  "type": "library",
  "homepage": "https://github.com/teodoroleckie/cache",
  "authors": [
    {
      "name": "Teodoro Leckie Westberg",
      "email": "teodoroleckie@gmail.com"
    }
  ],
  "keywords": [
    "tleckie",
    "php-couchbase",
    "couchbase",
    "couchbase-php",
    "php",
    "couchbase-cache",
    "PSR-6",
    "PSR-16",
    "php-8"
  ],
  "autoload": {
    "psr-4": {
      "Tleckie\\Cache\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tleckie\\Cache\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8",
    "ext-couchbase": "*",
    "ext-json": "*",
    "couchbase/couchbase": "^3.1",
    "psr/cache": "^1.0|^2.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "mikey179/vfsstream": "^1.6"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "vendor-dir": "vendor",
    "bin-dir": "bin"
  }
}
