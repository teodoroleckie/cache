{
  "name": "tleckie/cache",
  "description": "PSR-6 and PSR-16 Couchbase cache",
  "type": "library",
  "homepage": "https://github.com/teodoroleckie/cache",
  "authors": [
    {
      "name": "Teodoro Leckie Westberg",
      "email": "teodoroleckie@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Tleckie\\Cache\\": "src/"
    }
  },
  "require": {
    "ext-json": "*",
    "ext-couchbase": "*",
    "couchbase/couchbase": "^3.1",
    "psr/cache": "^1.0|^2.0",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5"
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "vendor-dir": "vendor",
    "bin-dir": "bin"
  }
}
